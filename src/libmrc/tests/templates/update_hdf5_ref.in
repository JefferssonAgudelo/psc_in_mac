
    for a in @abs_srcdir@/../reference_results/$TEST/*.h5.dump; do 
	b=`basename $a .dump`
	h5dump $b > $b.dump
	if sed 's/uid-0x[0-9a-f]*/uid/g' $b.dump | 
	   sed '/GROUP "mrc_vec-uid" {/,/^   }/d' |
    	   diff -b -I 'STRSIZE [0-9]\{1,2\}' -u $a -
	then
	    echo "${b} matches, not updating"
	else
	    echo "${b} doesn't match. Update?"
	    select yn in "Yes" "No"; do
		case $yn in
		    Yes ) sed 's/uid-0x[0-9a-f]*/uid/g' $b.dump |
		          sed '/GROUP "mrc_vec-uid" {/,/^   }/d' > $a; break;;
		    No ) break;;
		esac
	    done
	fi
    done
