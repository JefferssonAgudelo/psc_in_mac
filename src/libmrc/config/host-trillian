# trillan (PrgEnv-gnu)

if test -z "$CC"; then
   CC="cc -dynamic"
fi

if test -z "$F77"; then
   F77="ftn -dynamic"
fi

if test -z "$FC"; then
   FC="ftn -dynamic"
fi

if test -z "$CXX"; then
   CXX="CC -dynamic"
fi

if test -z "$FLIBS"; then
   FLIBS=" "
fi

if test -z "$CFLAGS"; then
   CFLAGS="-g -O3"
fi

if test -z "$FFLAGS"; then
   FFLAGS="-g -O3"
fi
