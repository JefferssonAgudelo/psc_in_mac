openmpirun -np 1 ../src/VLI \
	   --mod_output c \
	   --case collisions \
	   --mod_randomize fortran \
	   --mod_sort qsort \
	   --mod_collision fortran \
	   --itot_x 10 \
	   --ilo_x 0 --ihi_x 10 \
	   --itot_y 10 \
	   --ilo_y 0 --ihi_y 10 \
	   --itot_z 10 \
	   --ilo_z 0 --ihi_z 10 \
	   --output_format vtk \
	   --output_combine \
	   --write_tfield no \
	   --pfield_step 100 \
	   --nmax 2000 \
	   --nicell 2 \
	   --nproc_z 1 \
   	   --n0 1.0e30

